    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.android.payponseapp.pages.CameraPages">

    <com.dlazaro66.qrcodereaderview.QRCodeReaderView
        android:id="@+id/qrdecoderview"
        android:layout_width="match_parent"
